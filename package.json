{
  "name": "egg-RESTfulAPI",
  "version": "1.0.0",
  "description": "A optimized Node.js RESTful API Server Template based on egg.js!",
  "private": true,
  "dependencies": {
    "await-stream-ready": "^1.0.1",
    "egg": "^2.29.4",
    "egg-bcrypt": "^1.1.0",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-scripts": "^2.13.0",
    "egg-validate": "^2.0.2",
    "image-downloader": "^4.0.2",
    "mocha": "^8.4.0",
    "moment": "^2.29.1",
    "stream-to-array": "^2.3.0",
    "stream-wormhole": "^1.1.0"
  },
  "devDependencies": {
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.16.2",
    "egg-ci": "^1.18.0",
    "egg-mock": "^4.1.0",
    "eslint": "^7.27.0",
    "eslint-config-egg": "^9.0.0",
    "plop": "^2.7.4",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --ignore-stderr --sticky",
    "st": "egg-scripts start --env=st --daemon --ignore-stderr --sticky",
    "stop": "egg-scripts stop",
    "dev": "egg-bin dev",
    "dev2": "egg-bin dev --sticky --workers 2",
    "debug": "egg-bin debug",
    "check": "npm run lint -- --fix && npm run test",
    "test": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "new": "plop --plopfile plop-templates/plopfile.js",
